{
    "compilerOptions": {
        "noImplicitAny": false,
        "noEmitOnError": false,
        "removeComments": false,
        "sourceMap": true,
        "inlineSources": true,
        "target": "es5",
        "preserveConstEnums": true,
        "declaration": true,
        "module": "ESNext",
        "outDir": "../dist/ts",
        "lib": [
            "es2015",
            "dom",
            "webworker",
            "ES5",
            "ES6"
        ],
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "../src/**/*"
    ],
    "exclude": [
        "node_namespaces",
        "output"
    ]
}